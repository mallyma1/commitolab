{
  "expo": {
    "name": "Commito",
    "slug": "commito",
    "owner": "opsuma",
    "version": "1.0.0",
    "orientation": "portrait",
    "extra": {
      "eas": {
        "projectId": "e94a1503-cb55-46cb-a06d-3959f2358d4d"
      }
    },
    "icon": "./assets/images/icon.png",
    "scheme": "commito",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.commito.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Commito needs camera access to capture photo proof for your commitments.",
        "NSPhotoLibraryUsageDescription": "Commito needs photo library access to attach proof to your check-ins."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6366F1",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.commito.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6366F1",
          "dark": {
            "backgroundColor": "#1F2937"
          }
        }
      ],
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
