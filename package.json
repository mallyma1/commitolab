{
  "name": "my-app",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "expo:web": "EXPO_PUBLIC_DOMAIN=localhost:5000 npx expo start --web --port 8081",
    "expo:start": "EXPO_PUBLIC_DOMAIN=localhost:5000 npx expo start",
    "expo:go": "./start-expo-go.sh",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "all:dev": "npm run server:dev",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "lint": "eslint 'client/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint 'client/**/*.{ts,tsx,js,jsx}' --fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "echo \"No tests configured\" && exit 0",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write 'client/**/*.{ts,tsx,js,jsx,json,md}'",
    "ci:verify": "npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@google-cloud/storage": "^7.18.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@tanstack/react-query": "^5.90.7",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "expo": "~54.0.29",
    "expo-apple-authentication": "~8.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-blur": "~15.0.8",
    "expo-camera": "^17.0.10",
    "expo-constants": "~18.0.9",
    "expo-file-system": "^19.0.20",
    "expo-font": "~14.0.10",
    "expo-glass-effect": "~0.1.8",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.15",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "express": "^4.21.2",
    "google-auth-library": "^10.5.0",
    "http-proxy-middleware": "^3.0.5",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "stripe": "^20.0.0",
    "stripe-replit-sync": "^0.0.12",
    "tsx": "^4.20.6",
    "twilio": "^5.3.2",
    "ws": "^8.18.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
